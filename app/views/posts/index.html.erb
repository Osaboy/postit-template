<%= render 'shared/content_title', title: 'All Posts' %>
<!--no underscore before partial html template-->

<% @posts.each do |post| %>
  <%= render 'post', post: post %>
<% end %>

<%-#% if !@posts.empty? -%>
  <%-#= render @posts -%>
  <!-- This is how to render partial with collection -->
  <%-# render :partial => 'post', :collection => @posts -%>
<%-# end -%>
